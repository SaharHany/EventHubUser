<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.iti.eventhubuser">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".screens.splachscreen.SplachActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".screens.editprofilescreen.EditProfileActivity" />
        <activity android:name=".screens.feedbackscreen.FeedBackActivity" />
        <activity android:name=".screens.forgetpasswordscreen.ForgetPasswordActivity" />
        <activity android:name=".screens.hallscreen.HallActivity" />
        <activity android:name=".screens.loginscreen.LoginActivity" />
        <activity android:name=".screens.profilescreen.ProfileActivity" />
        <activity android:name=".screens.registerscreen.RegisterActivity" />
        <activity android:name=".screens.sessionscreen.SessionActivity" />
        <activity android:name=".screens.speakerscreen.SpeakerActivity" />
        <activity android:name=".screens.updatepasswordscreen.UpdatePasswordActivity" />
        <activity android:name=".screens.usereventsscreen.UserEventsActivity" />
    </application>

</manifest>